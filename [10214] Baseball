#include <iostream>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int T;
	cin >> T;
	while(T--)
	{
		int y=0,k=0;
		for(int i=0;i<9;i++)
		{
			int a=0,b=0;
			cin >> a >> b;
			y += a;
			k += b;
		}
		if(y==k)
			cout <<"Draw\n";
		else if(y>k)
			cout << "Yonsei\n";
		else
			cout << "Korea\n";
	}
	return 0;
}
