#include <iostream>
#include <algorithm>
using namespace std;

int main() {
	cin.tie(NULL);

	int T;
	cin >> T;
	for(int i=0;i<T;i++)
	{
		int R;
		char S[21];
		cin >> R >> S;
    
		for(int k=0; S[k] != '\0';k++)
			for(int i=0;i<R;i++)
				cout << S[k];
        
		cout << "\n";		
	}
	return 0;
}
