#include <stdio.h>
int main(void)
{
	int WantNum, i= 0;
	scanf("%d", &WantNum);
	int j = WantNum;
	int k = WantNum;
	int p = 0;
	int u = 0;
	for (i = 0; i < WantNum; i++)
	{
		for (; p<u;p++)
			printf(" ");
		for (; j>0; j--)
			printf("*");
		k--;
		j = k;
		u++;
		p = 0;
		
		printf("\n");
	}
	return 0;
}
