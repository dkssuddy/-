#include <iostream>
#include <algorithm>
#include <cmath>
using namespace std;

int main() {
	cin.tie(NULL);
	ios_base::sync_with_stdio(false);
	int A,B,N,aC=0,bC=0;
	cin >> N;
	for(int i=0;i<N;i++)
	{
		cin >> A >> B;
		if(A>B)
			aC++;
		else if(A<B)
			bC++;
	}
	cout << aC << " "<< bC;
	return 0;
}
