#include <iostream>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	
	int N;
	cin >> N;
	for(int i=0;i<N;i++)
	{
		for(int k=0;k<N;k++)
			k%2==0?cout << "*":cout <<" ";
		cout <<"\n";
		for(int p=0;p<N;p++)
			p%2==0?cout << " ":cout <<"*";
		cout << "\n";
	}
	
	return 0;
}
