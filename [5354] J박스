#include <iostream>
using namespace std;

int main()
{
	long long int T;
	cin >> T;
	
	while(T--)
	{
		int p;
		cin >> p;
		
		if(p==1)
		{
			cout << "#"<<endl;
			cout << endl;
			continue;
		}
		
		for(int i=0;i<p;i++)
			cout << "#";
		cout << endl;
			
		for(int i=0;i<p-2;i++)
		{
			cout << "#";
			for(int k=0;k<p-2;k++)
				cout << "J";
			cout << "#";	
			cout  << endl;
		}
    
		for(int i=0;i<p;i++)
			cout << "#";
		cout << endl;
		cout << endl;			
	}
	return 0;	
}
