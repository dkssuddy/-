#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
using namespace std;

const int dx[4]={ 1,0};
const int dy[4]={ 0,1};

int main() {
	cin.tie(NULL);
	ios_base::sync_with_stdio(false);

	for(;;)
	{
		string word;
		getline(cin,word);
		if(word=="END")
			break;
		reverse(word.begin(),word.end());
		cout <<word<<'\n';
	}
	return 0;
}
