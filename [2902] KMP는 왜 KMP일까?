#include <iostream>
#include <cmath>
#include <cstdlib>
#include <cstring>
#include <cstdio>
#include <string>
#include <vector>
using namespace std;

int main() {
	cin.tie(NULL);

	int count=0,i;
	string name;
		
	getline(cin,name);
	cout << name[0];
	for(i=0;name[i] !='\0';i++)
	{
		if(name[i]=='-')
			cout<< name[i+1];
	}
	return 0;
}
