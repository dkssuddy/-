#include <iostream>
#include <cmath>
#include <cstdlib>
#include <cstring>
#include <cstdio>
#include <string>
#include <vector>
using namespace std;

int main() {
	cin.tie(NULL);

	char word[101];
	int count=0;
	cin >> word;
	
	for(int i=0;word[i] !='\0';i++)
	{
		if(word[i]=='a' || word[i] == 'e' || word[i]=='i' || word[i]=='o' || word[i]== 'u')
		count++;
	}	
	cout << count;
	return 0;
}
