#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int T;
	cin >>T;
	while(T--)
	{
		vector <int> ans;
		for(int i=0;i<10;i++)
		{
			int a;
			cin >> a;
			ans.push_back(a);
		}
		sort(ans.begin(),ans.end());
		cout << ans.at(7) << "\n";
	}
	return 0;
}
