#include <iostream>
#define MODULAR 100007

using namespace std;
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(0); 
	cout.tie(0);
	int A,B,C,D;
	cin >> A>>B>>C>>D;
	
	C+=D;
	while(C>60)
	{
		C-=60;
		B++;
	}
	while(B>60)
	{
		B-=60;
		A++;
	}
	while(A>24)
		A-=24;
	cout<<A << " "<<B <<" "<< C;
	return 0;
}
