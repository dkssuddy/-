#include <iostream>
#include <string>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int N;
	cin >> N;
    cin.ignore();
    
	for(int i=0;i<=N;i++)
	{
		string word;
		getline(cin,word);
		if((int)word[0]>96)
			word[0] =(int)word[0]-32;
		cout << word <<'\n';
	}	
	return 0;	
}
